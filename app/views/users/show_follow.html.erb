<% provide(:title, @title) %>
<div class="row">
  <aside class="col-md-4">
    <section>
      <h1>
        <%= render 'shared/user_info' %>
      </h1>
    </section>
  </aside>
  <div class="col-md-8">
    <h3><%= @title %></h3>
    <% if @users.any? %>
      <% @users.each do |user| %>
        <div class="follow">
          <%= user.name %>
        </div>
      <% end %>
    <% end %>
  </div>
</div>